{
    "manifest_version": 2,
    "name": "yt-dl extension",
    "description": "The YoutubeDownloader Extension",
    "version": "1.0",
    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    },
    "permissions": [
        "activeTab",
        "http://yt-dl.furnocte.fr/*",
        "http://127.0.0.1:8080/*",
        "tabs"
    ]
}
